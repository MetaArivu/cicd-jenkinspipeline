node {

    stages {
        stage('Build') {
            steps {
                echo 'Build the App'
                 node -v
            }
        }
        stage('Test') {
            steps {
                echo 'Test the App'
            }
        }   
        stage('Staging') {
            steps {
                echo 'Deploy the App in Staging'

            }
        }
    }
}
