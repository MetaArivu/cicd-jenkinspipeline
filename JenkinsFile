pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build the App'
                sh 'mvn -v'
            }
        }
        stage('Test') {
            steps {
                echo 'Test the App'
            }
        }   
        stage('Staging') {
            steps {
                echo 'Deploy the App in Staging'

            }
        }
    }
}
